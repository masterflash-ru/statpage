{
    "name": "masterflash-ru/statpage",
    "description": "Просто страницы для системы управления Simba",
    "type": "Service",
    "license": "BSD-3-Clause",
    "keywords": [
        "masterflash",
        "simba"
    ],
    "homepage": "https://masterflash.ru/",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2 || ^8.0",
        "laminas/laminas-cache": "*",
        "laminas/laminas-serializer": "*",
        "masterflash-ru/ado": "*"
    },
    "autoload": {
        "psr-4": {
            "Mf\\Statpage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mf\\Statpage\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev",
            "dev-develop": "1.1-dev"
        },
        "zf": {
            "component": "Mf\\Statpage",
            "config-provider": "masterflash-ru\\Mf\\Statpage\\ConfigProvider"
        }
    }
}
